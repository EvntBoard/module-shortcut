  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EvntBoard/module-shortcut.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EvntBoard/module-shortcut/issues"
  },
  "homepage": "https://github.com/EvntBoard/module-shortcut#readme",
  "dependencies": {
    "evntcom-js": "https://github.com/EvntBoard/evntcom-js.git",
    "iohook": "^0.9.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.6.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.0",
    "ts-node": "^10.2.1",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.4.3"
  },
  "iohook": {
    "targets": [
      "node-83"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
