{
  "name": "module-shortcut",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "scripts": {
    "start": "nodemon --watch \"src/**/*.ts\" --exec \"ts-node\" src/index.ts",
    "build": "ncc build src/index.ts -m -o dist/module",
    "lint": "prettier --write src",
